метод Кнопка1ПриНажатии(Источник: Кнопка, Событие: СобытиеПриНажатии)
    знч Куда= Компоненты.Группа1.Содержимое
    
    для ПредставлениеКлюча из НаСервере()
        Куда.Добавить(новый Надпись(Значение = ПредставлениеКлюча))
    ;
;

@НаСервере @ДоступноСКлиента
статический метод НаСервере(): Массив<Строка>
    знч ВсеКлючи = КлючиДоступа.ВМассив()
    
    знч ВИА = Безопасность.ВстроенныеИдАутентификации
    
    возврат ВсеКлючи.Преобразовать( К -> К.Представление() )
;