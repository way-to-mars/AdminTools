@Обработчик
метод ПослеСоздания()
    Компоненты.ГруппаСтрок.Содержимое.ДобавитьВсе(МассивСтрок())
    ДоступностьДействий = Истина
;

метод МассивСтрок(): Массив<КомпонентСтрокаСпискаИзменений>
    возврат СписокДействий.Преобразовать( КиЗ ->
        новый КомпонентСтрокаСпискаИзменений(
            ИмяДействия = КиЗ.Ключ,
            Действие = КиЗ.Значение
        ))
;