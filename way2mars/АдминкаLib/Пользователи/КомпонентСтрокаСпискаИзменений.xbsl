@ВПодсистеме
метод ПрименитьИзменение()
    если Выбор
        Статус = СтатусИзменения.Запущен
        знч Результат = Действие()
        
        если Результат == Неопределено
            Статус = СтатусИзменения.Успех
        иначе
            Статус = СтатусИзменения.Ошибка
            знч Сообщение = Результат как Строка
               
        ;
    ;
;